<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="script.module.hostconfig" name="Host Modification" version="3.2.5" provider-name="NewfieRocket">
	<requires>
		<import addon="xbmc.python" version="2.14.0"/>
	</requires>
	<extension point="xbmc.python.script" library="default.py">
		<provides>executable</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<platform>all</platform>
		<summary lang="en">Modify Hosts file for use with LazyMan</summary>
		<description lang="en">Modify Host</description>
		<license>GNU General Public License, v2</license>
		<language></language>
		<assets>
			<icon>icon.png</icon>
			<fanart>fanart.jpg</fanart>
		</assets>

	</extension>
</addon>

<addon id="plugin.video.newfierocket" name="NewfieRocket" version="0.1" provider-name="NewfieRocket">
    <requires>
        <import addon="xbmc.python" version="2.19.0" />
        <import addon="script.module.nanscrapers" version="0.5.5" />
        <import addon="script.module.python.koding.aio" version="0.9.6.6" />
        <import addon="script.module.beautifulsoup" version="3.2.1" />
        <import addon="script.module.requests" version="2.9.1" />
        <import addon="script.module.addon.common" version="2.0.0" />
        <import addon="script.module.xmltodict" version="0.9.0" />
        <!--<import addon="script.module.web-pdb" version="1.0"/>-->
        <import addon="script.module.urlresolver" version="1.2.0" />
    </requires>
    <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>audio video</provides>
    </extension>
    <extension point="xbmc.service" library="service.py" start="[login|startup]" />
    <extension point="xbmc.addon.metadata">
        <summary lang="en">Bob's little sister</summary>
        <description lang="en">Bob's little sister</description>
        <platform>all</platform>
        <website>www.noobsandnerds.com</website>
        <video_guide>xtOqLY5PJDY</video_guide>
    </extension>
</addon>
</addons>
