<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="script.module.hostconfig" name="Host Modification" version="3.2.3" provider-name="NewfieRocket">
	<requires>
		<import addon="xbmc.python" version="2.14.0"/>
	</requires>
	<extension point="xbmc.python.script" library="default.py">
		<provides>executable</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<platform>all</platform>
		<summary lang="en">Modify Hosts file for use with LazyMan</summary>
		<description lang="en">Modify Host</description>
		<license>GNU General Public License, v2</license>
		<language></language>
		<assets>
			<icon>icon.png</icon>
			<fanart>fanart.jpg</fanart>
		</assets>

	</extension>
</addon>

<addon id="repository.newfierocket" name="NewfieRocket" version="2.0.0" provider-name="NewfieRocket">
	<extension point="xbmc.addon.repository" name="NewfieRocket">
		<dir>
			<info compressed="false">https://raw.githubusercontent.com/noobsandnerds/modules4all/master/zips/addons.xml</info>
			<checksum>https://raw.githubusercontent.com/noobsandnerds/modules4all/master/zips/addons.xml.md5</checksum>
			<datadir zip="true">https://raw.githubusercontent.com/noobsandnerds/modules4all/master/zips/</datadir>
		</dir>
		<info compressed="false">https://raw.githubusercontent.com/newfierocket/repository.newfierocket/master/zips/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/newfierocket/repository.newfierocket/master/zips/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/newfierocket/repository.newfierocket/master/zips/</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Videos and things</summary>
		<description lang="en">Videos and things</description>
		<genre>movies,canadian,usa</genre>
	</extension>
</addon>

<addon id="repository.lazyman" name="LazyMan Repo" version="1.0.0" provider-name="reddit.com/r/lazyman">
  <extension point="xbmc.addon.repository" name="LazyMan Repo">
      <info compressed="true">http://raw.githubusercontent.com/actionbronson/LazyMan/master/src/kodi/repository/addons.xml.gz</info>
      <checksum>http://raw.githubusercontent.com/actionbronson/LazyMan/master/src/kodi/repository/addons.xml.gz.md5</checksum>
      <datadir zip="true">http://raw.githubusercontent.com/actionbronson/LazyMan/master/src/kodi/repository/</datadir>
  </extension>
  <extension point="xbmc.addon.metadata">
      <summary>Install LazyMan Addons</summary>
      <description>Download and install LazyMan addons</description>
      <platform>all</platform>
  </extension>
</addon>
</addons>
